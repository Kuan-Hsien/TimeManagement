<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintlayout_icon_item"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingTop="@dimen/padding_icon_item"
    android:paddingBottom="@dimen/padding_icon_item"
    android:clickable="true"
    android:focusable="true" >

    <!-- android:background="@drawable/shape_layout_white_radius_card" -->

    <ImageView
        android:id="@+id/imageview_icon_item"
        android:layout_width="@dimen/size_icon_item"
        android:layout_height="@dimen/size_icon_item"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" />

</android.support.constraint.ConstraintLayout>
